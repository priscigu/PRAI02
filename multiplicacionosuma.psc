	Escribir 'Ingrese el primer número'
	Leer num1
	Escribir 'Ingrese el segundo número'
	Leer num2
	Escribir 'Ingrese el tercer número'
	Leer num3
	mult=num1*num2*num3
	suma=num1+num2+num3
	Si num1<0 Entonces
		Escribir 'EL RESULTADO ES: ',mult 
	SiNo
		Escribir 'eL RESULTADO ES: ',suma
	Fin Si
FinAlgoritmo
